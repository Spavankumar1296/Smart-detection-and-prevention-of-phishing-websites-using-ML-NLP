{
    "manifest_version": 3,
    "name": "PhishGuard AI",
    "version": "1.0",
    "description": "Real-time Phishing Detection Extension",
    "permissions": [
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "PhishGuard AI"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}